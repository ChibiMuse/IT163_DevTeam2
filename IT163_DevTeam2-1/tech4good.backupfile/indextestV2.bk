body {
 
    background: rgb(255, 255, 255);
}

 
header {
  width: 100%;
  display: grid;
  grid-column: 1 / 4;
  grid-row: 2 / 3;
  background-color: rgb(255, 255, 255);
 
}


.wrapper {
  width: auto;
    overflow: hidden;
}
h2 {
  display: grid;
  background-color: rgb(255, 255, 255);
  font-family: "Helvetica Neue", Arial, Helvetica, sans-serif;
  color: rgb(32, 19, 19);
  grid-column: 1 / 4;
  grid-row: 3 / 4;
  font-size: 20ox;
}

main h2 {
  display: grid;
  background-color: rgb(255, 255, 255);
  font-family: "Helvetica Neue", Arial, Helvetica, sans-serif;
  grid-column: 1 / 4;
  grid-row: 3 / 4;
}

footer {
  background-color: rgb(255, 255, 255);
  color: rgb(32, 19, 19);
  font: 10px "Helvetica Neue", Arial, Helvetica, sans-serif;
  text-align: center;
  grid-row: 4 / 5;
  grid-column: 1 / 5;
}

 nav {
   display: grid;
   grid-column: 1 / 4;
   grid-row:  1 / 2;
   background-color: rgb(255, 255, 255);
    width: 100%;
    justify-items: center;
    font-family: "Helvetica Neue", Arial, Helvetica, sans-serif; 
 }

 ul.topnav li   {
  display: inline-block;
  margin: 20px;
  font-style: black 30px;
  font-family: "Helvetica Neue", Arial, Helvetica, sans-serif; 

}

:hover nav ul li a:hover {

  background: #969494; /* Old browsers */

