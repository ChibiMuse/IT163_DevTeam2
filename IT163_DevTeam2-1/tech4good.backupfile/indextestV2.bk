  }

  /* extra large screens */

  @media all and (min-width: 1400px) {

    #mobi {
      display: none;
    }

    img.desktop {
      width: 100%;
      display: inline-block;
     }
    
     img.tablet, img.phone {
        display: none;
    }
      main {
       width:65%;
     } 
  }

  #myLinks  {
    display: none;
  }

