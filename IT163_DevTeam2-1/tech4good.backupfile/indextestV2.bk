/*TABLET*/
@media all and (min-width:738px) and (max-width: 920px) {

  main {
    width: 75%;
    overflow: hidden; 
  }

  #mobi {
    display: block;
  }
 
  #myTopnav
  {
    display: none;
  }

  nav {
    display: none;

  }
  

/* changed wisth from auito and block to inline  */
  img.tablet {
          width: 100%;
          display: inline-block;
   }

  img.desktop, img.phone {
        display: none;
     }  


