   } */

}

/* PHONE */
@media all and (max-width: 737px) {

  main {
    width: 100%;
    margin: 10px;
    overflow: hidden; 
  }
  
  .wrapper #mobi {
    display: block;
  }

  #myTopnav
  {
    display: none;
  }

  nav {
    display: none;
  }

  /* changed from block-size */
  img.phone {
          width:280px;
          display: block;
   }

   img.tablet, img.desktop {
        display: none;
     }  
    }
   
    /* FORM CSS */
    main.form  {
      margin:1em;
      font: 14px "Fieldwork Geo", Acute, Helvetica;
    }
